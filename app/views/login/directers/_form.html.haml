  -#       =f.check_box_without_bootstrap :remove_image
  -#       =f.label :remove_image, 'Image Delete'
  -# =f.collection_select :directer_id, Directer.all, :id, :name, include_blank: true, class: 'form-control '
  -# =f.submit
  =f.submit